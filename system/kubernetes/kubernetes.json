[{
  "apiVersion" : "v1",
  "kind" : "Service",
  "metadata" : {
    "annotations" : {
      "app.quarkus.io/quarkus-version" : "3.11.0",
      "app.quarkus.io/vcs-uri" : "https://github.com/soybeanjs/soybean-admin-quarkus.git"
    },
    "labels" : {
      "app.kubernetes.io/name" : "system",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "system",
    "namespace" : "default"
  },
  "spec" : {
    "ports" : [ {
      "name" : "https",
      "port" : 443,
      "protocol" : "TCP",
      "targetPort" : 8443
    }, {
      "name" : "http",
      "port" : 80,
      "protocol" : "TCP",
      "targetPort" : 8080
    } ],
    "selector" : {
      "app.kubernetes.io/name" : "system"
    },
    "type" : "ClusterIP"
  }
},{
  "apiVersion" : "apps/v1",
  "kind" : "Deployment",
  "metadata" : {
    "annotations" : {
      "app.quarkus.io/quarkus-version" : "3.11.0",
      "app.quarkus.io/vcs-uri" : "https://github.com/soybeanjs/soybean-admin-quarkus.git"
    },
    "labels" : {
      "app.kubernetes.io/name" : "system",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "system",
    "namespace" : "default"
  },
  "spec" : {
    "replicas" : 1,
    "selector" : {
      "matchLabels" : {
        "app.kubernetes.io/name" : "system"
      }
    },
    "template" : {
      "metadata" : {
        "annotations" : {
          "app.quarkus.io/quarkus-version" : "3.11.0",
          "app.quarkus.io/vcs-uri" : "https://github.com/soybeanjs/soybean-admin-quarkus.git"
        },
        "labels" : {
          "app.kubernetes.io/managed-by" : "quarkus",
          "app.kubernetes.io/name" : "system"
        },
        "namespace" : "default"
      },
      "spec" : {
        "containers" : [ {
          "env" : [ {
            "name" : "KUBERNETES_NAMESPACE",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "metadata.namespace"
              }
            }
          } ],
          "image" : "harbor.my-website.com/library/system:1.0.0-SNAPSHOT",
          "imagePullPolicy" : "Always",
          "livenessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/live",
              "port" : 8080,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "name" : "system",
          "ports" : [ {
            "containerPort" : 8443,
            "name" : "https",
            "protocol" : "TCP"
          }, {
            "containerPort" : 8080,
            "name" : "http",
            "protocol" : "TCP"
          } ],
          "readinessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/ready",
              "port" : 8080,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 20,
            "periodSeconds" : 45,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "resources" : {
            "limits" : {
              "cpu" : "1000m",
              "memory" : "512Mi"
            },
            "requests" : {
              "cpu" : "250m",
              "memory" : "64Mi"
            }
          },
          "startupProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/started",
              "port" : 8080,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          }
        } ]
      }
    }
  }
},{
  "apiVersion" : "networking.k8s.io/v1",
  "kind" : "Ingress",
  "metadata" : {
    "annotations" : {
      "app.quarkus.io/quarkus-version" : "3.11.0",
      "app.quarkus.io/vcs-uri" : "https://github.com/soybeanjs/soybean-admin-quarkus.git",
      "cert-manager.io/cluster-issuer" : "dnspod",
      "kubernetes.io/ingress.class" : "nginx",
      "nginx.ingress.kubernetes.io/force-ssl-redirect" : "true",
      "nginx.ingress.kubernetes.io/rewrite-target" : "/$1"
    },
    "labels" : {
      "app.kubernetes.io/name" : "system",
      "app.kubernetes.io/managed-by" : "quarkus"
    },
    "name" : "system",
    "namespace" : "default"
  },
  "spec" : {
    "rules" : [ {
      "host" : "api.my-website.com",
      "http" : {
        "paths" : [ {
          "backend" : {
            "service" : {
              "name" : "system",
              "port" : {
                "name" : "http"
              }
            }
          },
          "path" : "/v1/(.*)",
          "pathType" : "Prefix"
        } ]
      }
    } ],
    "tls" : [ {
      "hosts" : [ "api.my-website.com" ],
      "secretName" : "soybean-tls"
    } ]
  }
}]